        </Datasource>
    </Layer>
    <Layer name="GeneralLandArea" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>GeneralLandArea</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">general_land_area</Parameter>
        </Datasource>
    </Layer>
    <Layer name="GeneralLandLine" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>GeneralLandLine</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">general_land_area_line</Parameter>
        </Datasource>
    </Layer>
    <Layer name="GeneralCoastline" srs="+proj=latlong +datum=WGS84" status="on">
