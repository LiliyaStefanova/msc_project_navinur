        </Datasource>
    </Layer>
    <Layer name="GeneralDepthArea" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>GeneralDepthArea</StyleName>
        <StyleName>depthAreaLabel</StyleName>
        <Datasource>
