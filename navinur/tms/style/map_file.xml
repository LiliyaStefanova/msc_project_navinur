    </Layer>
    <Layer name="OverviewCoastLine" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>OverviewCoastLine</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
