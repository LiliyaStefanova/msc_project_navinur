            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_wreck_point</Parameter>
        </Datasource>
    </Layer>
    <Layer name="OverviewBuoy" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>OverviewBuoy</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
