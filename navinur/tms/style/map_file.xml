<!DOCTYPE Map>
<Map srs="+proj=latlong +datum=WGS84" background-color="#FFFFFF" minimum-version="0.7.2">
    <Style name="OceanBase">
        <Rule>
            <PolygonSymbolizer fill="#5F9EA0"/>
        </Rule>
    </Style>
    <Style name="CoastalCoverageArea">
    <Rule>
    <LineSymbolizer stroke="#00FF00" stroke-width="0.5"/>
    </Rule>
    </Style>
    <Style name="CoastalCoverageAreaTitle">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <TextSymbolizer allow-overlap="no" size="8" face-name="DejaVu Sans Book" halo-fill="#8C001A">
                [title]
            </TextSymbolizer>
        </Rule>
    </Style>
    <Style name="GeneralCoverageArea">
        <Rule>
            <LineSymbolizer stroke="#E8A317" stroke-width="1"/>
        </Rule>
    </Style>
    <Style name="BaseLayer">
        <Rule>
            <PolygonSymbolizer fill="#F0E68C"/>
            <LineSymbolizer stroke="#404040" stroke-width="0.2"/>
        </Rule>
    </Style>
    <Style name="BaseLayerText">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <TextSymbolizer size="12" wrap-width="15" face-name="DejaVu Sans Book" halo-fill="#000000">[name]</TextSymbolizer>
        </Rule>
    </Style>
    <Style name="tempLandPoly">
        <Rule>
            <PolygonSymbolizer fill="#FFFFFF"/>
        </Rule>
    </Style>
    <Style name="OverviewLandArea">
        <Rule>
            <PolygonSymbolizer fill="#F0E68C"/>
        </Rule>
    </Style>
    <Style name="GeneralLandArea">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#F0E68C"/>
        </Rule>
    </Style>
    <Style name="CoastalLandArea">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#F0E68C"/>
        </Rule>
    </Style>
    <Style name="OverviewLandLine">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralLandLine">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="CoastalLandLine">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewCoastline">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralCoastline">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="CoastalCoastline">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewDepthArea">
        <Rule>
            <Filter>[drval1] &gt;= 0</Filter>
            <PolygonSymbolizer fill="#5F9EA0"/>
        </Rule>
    </Style>
    <Style name="GeneralDepthArea">
        <Rule>
            <Filter>[drval1] &gt;= 0</Filter>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#5F9EA0" gamma="0.63"/>
            <TextSymbolizer size="8" face-name="DejaVu Sans Book" halo-fill="#000000" allow-overlap="no">[drval1]
            </TextSymbolizer>
        </Rule>
    </Style>
    <Style name="CoastalDepthArea">
        <Rule>
            <Filter>[drval1] &gt;= 0</Filter>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#5F9EA0"/>
        </Rule>
    </Style>
    <Style name="DryingHeight">
    <Rule>
    <Filter>[drval1] &lt;= 0</Filter>
    <MaxScaleDenominator>18000000</MaxScaleDenominator>
    <PolygonSymbolizer fill="#CFD784"/>
    </Rule>
    </Style>
    <Style name="DepthAreaText">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <TextSymbolizer size="8" face-name="DejaVu Sans Book" halo-fill="#000000" avoid-edges="true"
                            allow-overlap="no">[drval1]
            </TextSymbolizer>
        </Rule>
    </Style>
    <Style name="OverviewDepthContour">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralDepthContour">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="CoastalDepthContour">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewRiverLine">
        <Rule>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.5"/>
        </Rule>
    </Style>
    <Style name="GeneralRiverLine">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.5"/>
            <TextSymbolizer size="8" face-name="DejaVu Sans Book" halo-fill="#000000">[objnam]</TextSymbolizer>
        </Rule>
    </Style>
    <Style name="CoastalRiverLine">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.5"/>
            <TextSymbolizer size="8" allow-overlap="no" face-name="DejaVu Sans Book" halo-fill="#000000">
                [objnam]
            </TextSymbolizer>
        </Rule>
    </Style>
    <Style name="OverviewWreckPoint">
        <Rule>
            <PointSymbolizer allow-overlap="no" file="../img/visible wreck.png"/>
        </Rule>
    </Style>
    <Style name="GeneralWreckPoint">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no" file="../img/visible wreck.png"/>
        </Rule>
    </Style>
    <Style name="CoastalWreckPoint">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no" file="../img/visible wreck.png"/>
        </Rule>
    </Style>
    <Style name="OverviewBuoy">
        <Rule>
            <PointSymbolizer allow-overlap="no" file="../img/beacon_special_purpose_general_p.png"/>
        </Rule>
    </Style>
    <Style name="GeneralBuoy">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no" file="../img/beacon_special_purpose_general_p.png"/>
        </Rule>
    </Style>
    <Style name="CoastalBuoy">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no" file="../img/beacon_special_purpose_general_p.png"/>
        </Rule>
    </Style>
    <Style name="CoastalAnchorageArea">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke-dasharray="1.1, 1.1" stroke="#FF00FF" stroke-width="1"/>
        </Rule>
    </Style>
    <Style name="GeneralLandRegionText">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <TextSymbolizer allow-overlap="no" size="8" face-name="DejaVu Sans Book" halo-fill="#000000">
                [objnam]</TextSymbolizer>
        </Rule>
    </Style>
    <Style name="grid">
        <Rule>
            <LineSymbolizer stroke="#000000" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="Route">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#8C001A" stroke-width="4" stroke-linecap="round" stroke-linejoin="round"/>
            <TextSymbolizer size="9"  fill="#FFFFFF" face-name="DejaVu Sans Book"
                            halo-fill="#FFFFFF" placement="line">[distance]</TextSymbolizer>
        </Rule>
    </Style>
    <Style name="RouteDirection">
        <Rule>
            <MaxScaleDenominator>25000000</MaxScaleDenominator>
            <MarkersSymbolizer allow-overlap="yes" marker-type="arrow" placement="line" fill="#8C001A"/>
        </Rule>
    </Style>
    <Style name="RouteIcons">
        <Rule>
            <MaxScaleDenominator>18000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="yes" file="../img/circle-dark-icon.png"/>
        </Rule>
    </Style>
</Map>


