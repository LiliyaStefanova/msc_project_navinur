<!DOCTYPE Map>
<Map srs="+proj=latlong +datum=WGS84" background-color="#FFFFFF" minimum-version="0.7.2">
    <Style name="baseLayer">
        <Rule>
            <PolygonSymbolizer fill="#b5d19c"/>
            <LineSymbolizer stroke="#404040" stroke-width="0.2"/>
        </Rule>
    </Style>
    <Style name="landArea">
        <Rule>
            <PolygonSymbolizer fill="#F0E68C" gamma="0.63"/>
        </Rule>
    </Style>
    <Style name="coastLine">
        <Rule>
            <LineSymbolizer stroke="#000000" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="depthArea">
        <Rule>
            <PolygonSymbolizer fill="#5F9EA0" gamma="0.63"/>
        </Rule>
    </Style>

    <Style name="depthContour">
        <Rule>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="landRegion">
        <Rule>
            <TextSymbolizer size="10" face-name="DejaVu Sans Book" halo-fill="#000000">[objnam]</TextSymbolizer>
        </Rule>
    </Style>
    <Layer name="baseMap" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>baseLayer</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">tms_basemap</Parameter>
        </Datasource>
    </Layer>
    <Layer name="landArea" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>landArea</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_land_area</Parameter>
        </Datasource>
    </Layer>
    <Layer name="landAreaLine" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>coastLine</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_land_area_line</Parameter>
        </Datasource>
    </Layer>
    <Layer name="depthContour" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>depthContour</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_depth_contour_line</Parameter>
        </Datasource>
    </Layer>
    <Layer name="depthArea" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>depthArea</StyleName>
        <StyleName>depthAreaLabel</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_depth_area</Parameter>
        </Datasource>
    </Layer>
    <Layer>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_land_region_point</Parameter>
        </Datasource>
    </Layer>

</Map>


