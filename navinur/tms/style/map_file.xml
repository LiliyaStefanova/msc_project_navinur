<!DOCTYPE Map>
<Map srs="+proj=latlong +datum=WGS84" background-color="#FFFFFF" minimum-version="0.7.2">
    <Style name="baseLayer">
        <Rule>
            <PolygonSymbolizer fill="#F0E68C"/>
            <LineSymbolizer stroke="#404040" stroke-width="0.2"/>
        </Rule>
    </Style>
    <Style name="baseLayerText">
        <Rule>
             <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <TextSymbolizer size="8" face-name="DejaVu Sans Book" halo-fill="#000000">[name]</TextSymbolizer>
        </Rule>
    </Style>

    <Style name="OverviewLandArea">
        <Rule>
            <PolygonSymbolizer fill="#F0E68C" gamma="0.63"/>
        </Rule>
    </Style>
    <Style name="GeneralLandArea">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#F0E68C" gamma="0.63"/>
        </Rule>
    </Style>
    <Style name="OverviewLandLine">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralLandLine">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewCoastline">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralCoastline">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewDepthArea">
        <Rule>
            <PolygonSymbolizer fill="#5F9EA0" gamma="0.63"/>
        </Rule>
    </Style>
    <Style name="GeneralDepthArea">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <PolygonSymbolizer fill="#5F9EA0" gamma="0.63"/>
        </Rule>
    </Style>
    <Style name="OverviewDepthContour">
        <Rule>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="GeneralDepthContour">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#404040" stroke-width="0.1"/>
        </Rule>
    </Style>
    <Style name="OverviewRiverLine">
        <Rule>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.5"/>
        </Rule>
    </Style>
    <Style name="GeneralRiverLine">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <LineSymbolizer stroke="#5F9EA0" stroke-width="0.5"/>
        </Rule>
    </Style>
    <Style name="OverviewWreckPoint">
        <Rule>
            <PointSymbolizer allow-overlap="no" file="/Users/liliya/repos/navinur/navinur/tms/img/visible wreck.png"/>
        </Rule>
    </Style>
    <Style name="GeneralWreckPoint">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no" file="/Users/liliya/repos/navinur/navinur/tms/img/visible wreck.png"/>
        </Rule>
    </Style>
    <Style name="OverviewBuoy">
        <Rule>
            <PointSymbolizer allow-overlap="no"
                             file="/Users/liliya/repos/navinur/navinur/tms/img/beacon_special_purpose_general_p.png"/>
        </Rule>
    </Style>
    <Style name="GeneralBuoy">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <PointSymbolizer allow-overlap="no"
                             file="/Users/liliya/repos/navinur/navinur/tms/img/beacon_special_purpose_general_p.png"/>
        </Rule>
    </Style>
    <Style name="anchorage">
        <Rule>
            <!-- <MaxScaleDenominator>6000000</MaxScaleDenominator>-->
            <!-- <MinScaleDenominator>700000</MinScaleDenominator> -->
            <PointSymbolizer allow-overlap="no" file="/Users/liliya/repos/navinur/navinur/tms/img/anchorage.png"/>
        </Rule>
    </Style>
    <Style name="GeneralLandRegionText">
        <Rule>
            <MaxScaleDenominator>20000000</MaxScaleDenominator>
            <TextSymbolizer size="8" face-name="DejaVu Sans Book" halo-fill="#000000">[objnam]</TextSymbolizer>
        </Rule>
    </Style>
    <!--this layer will remain as a base regardless -->
    <Layer name="baseMap" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>baseLayer</StyleName>
        <StyleName>baseLayerText</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">tms_basemap</Parameter>
        </Datasource>
    </Layer>

    <Layer name="OverviewLandArea" srs="+proj=latlong +datum=WGS84" status="on">
        <StyleName>OverviewLandArea</StyleName>
        <Datasource>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="dbname">navinur_db</Parameter>
            <Parameter name="user">liliya</Parameter>
            <Parameter name="table">overview_land_area</Parameter>
        </Datasource>
    </Layer>
    <Layer name="OverviewLandLine" srs="+proj=latlong +datum=WGS84" status="on">
